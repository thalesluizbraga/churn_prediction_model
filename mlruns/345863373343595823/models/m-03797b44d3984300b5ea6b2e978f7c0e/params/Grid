GridSearchCV(cv=3,
             estimator=RandomForestClassifier(max_depth=10, n_jobs=-1,
                                              random_state=42),
             param_grid={'max_depth': [10, 20], 'min_samples_leaf': [1, 2, 3],
                         'min_samples_split': [2, 4, 6],
                         'n_estimators': [100, 200, 250, 300, 500]},
             scoring='roc_auc', verbose=4)